extends layout
include includes/parameterDocMixins

block content
  header.jumbotron.subhead
    div.container
      h1 OIS
      div.page-header
        p.lead OIS dokumentation.
    div.container
      div.row
        div.span3
          ul#indhold.nav.nav-list
           li(data-toggle='collapse', data-target='#ois').nav-header OIS API
            ul#ois.nav.nav-list
              - each oisEntityName in docUtil.getOisEntityNames()
                li
                  a(href='#ois_' + oisEntityName) #{docUtil.getOisPlural(oisEntityName)}

        div.span9
          section
            - each oisEntityName in docUtil.getOisEntityNames()
              h3.anchor-header(id='ois_' + oisEntityName) OIS #{docUtil.getOisPlural(oisEntityName)} s√∏gning
              +documentOisQueryResource(oisEntityName)
              +documentOisGetByKeyResource(oisEntityName)
block append scripts
